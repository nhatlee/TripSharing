# platform :ios, '9.0'

def shared_pods
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'IQKeyboardManagerSwift'
end


target 'TripSharing' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  shared_pods

  # Pods for TripSharing

  target 'TripSharingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TripSharingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
